# Telegram Bot Configuration
BOT_TOKEN=YOUR_BOT_TOKEN

# Database Configuration (must match docker-compose.yml)
DATABASE_URL=postgres://postgres:sheralibot@db:5432/sherali_datadb

# Server Configuration
PORT=3000
NODE_ENV=production

# Screenshot Configuration (8 hours = 28800000ms)
SCREENSHOT_CACHE_DURATION=28800000
SCREENSHOT_QUALITY=95
PUPPETEER_CONCURRENCY=3

# Logging Configuration
LOG_LEVEL=log
